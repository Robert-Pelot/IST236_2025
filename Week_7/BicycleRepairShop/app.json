{
  "expo": {
    "name": "SandwichShop",
    "slug": "SandwichShop",
    "version": "1.0.0",
    "orientation": "portrait",
    //"icon": "./assets/images/Biker.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/Biker.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
    },
    "plugins": [
      "expo-router"
    ]
  }
}